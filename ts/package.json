  "repository": {
    "type": "git",
    "url": "https://github.com/zeenea/common-properties.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "typescript": "~5.3.3"
  }
}
